    "type": "module",
    "scripts": {
        "dev": "cross-env NODE_ENV=development tsx watch src/main.ts",
        "start": "cross-env NODE_ENV=production yarn build && node dist/main.js",
        "prebuild": "yarn db:generate",
        "build": "yarn prebuild && yarn tsc && tsc-alias",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "format": "prettier --write src/**/*.ts",
        "db:generate": "prisma generate",
        "db:studio": "prisma studio",
        "db:pull": "prisma db pull && yarn db:generate",
        "test": "echo \"Error: no test specified\" && exit 1"
